<div class="headline">
<div class="container">
<h1>Members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Andrew</th>
    <th>Year</th>
    <th>Title</th>
  </tr>
  <% for member in @members %>
    <tr>
      <td><%= member.name %></td>
      <td><%= member.andrew_id %></td>
      <td><%= member.year %></td>
      <td><%= member.title %></td>
      <td><%= link_to "Show", member %></td>
      <td><%= link_to "Edit", edit_member_path(member) %></td>
      <td><%= link_to "Destroy", member, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<% if current_user.role == "admin" %>
<p><%= link_to "New Member", new_member_path %></p>
<% end %>
</div>
</div>
