<div class="headline">
<div class="container">
<h1>Login</h1>
<%= form_tag sessions_path do %>
  <dl><dt>
    <%= label_tag :login, "Andrew ID" %></dt>
<dd>    <%= text_field_tag :login, params[:login] %></dd>
    <dt>
    <%= label_tag :password %></dt>
<dd>    <%= password_field_tag :password %></dd>
  </dl>
  <%= submit_tag "Log in" %>
<% end %>
</div>
</div>
